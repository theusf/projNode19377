<br><h1>Listagem de CLIENTES</h1><br><br>

<div>
<table class="table">
<thead class="thead-dark">
  <tr>
     <th>idProd</th>
     <th>Produto</th>
     <th>id Categoria</th>
     <th>Descrição</th>
     <th>Valor</th>
  </tr>
  </thead>
  <tbody>
  <tr for (produto in data.produtos)>
     <td>${produto.idProd}</td>
     <td>${produto.descProd}</td>
     <td>${produto.idCatProd}</td>
     <td>${produto.descTecProd}</td>
     <td>${produto.valorProd}</td>
  </tr>
  </tbody>
</table>
<script src="./removeCliente.js"></script>
